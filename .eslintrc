{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "max-depth": [
            "error",
            2
        ],
        "no-console": "warn",
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "linebreak-style": "off",
        "import/extensions": "off",
        "no-alert": "off",
        "operator-linebreak": "off",
        "comma-dangle": "off",
        "object-curly-newline": "off",
        "class-methods-use-this": "off",
        "implicit-arrow-linebreak": "off",
        "consistent-return": "off",
        "object-shorthand": "off"
    }
}
